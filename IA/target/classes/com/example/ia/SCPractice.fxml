<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1080.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ia.ControllerSCPractice">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="751.0" prefWidth="1040.0">
      
      <children>

         <Label fx:id="welcomeText" layoutX="362.0" layoutY="18.0" text="Single Chord Practice">
            <font>
               <Font size="26.0" />
            </font>
         </Label>
         <Button mnemonicParsing="false" onAction="#backtomenu" prefHeight="42.0" prefWidth="83.0" text="Back">
            <font>
               <Font size="16.0" />
            </font>
         </Button>

         <ImageView fx:id="SelectedImage" fitHeight="217.0" fitWidth="290.0" layoutX="29.0" layoutY="124.0" pickOnBounds="true" preserveRatio="true" />
         <Button layoutX="628.0" layoutY="288.0" mnemonicParsing="false" onAction="#startPractice" prefHeight="62.0" prefWidth="121.0" text="Click to begin">
            <font>
               <Font size="16.0" />
            </font>
         </Button>

         <Label fx:id="NoteView" layoutX="484.0" layoutY="423.0" prefHeight="27.0" prefWidth="245.0">
            <font>
               <Font size="18.0" />
            </font>
         </Label>
         <ListView fx:id="displayFeedback" layoutX="29.0" layoutY="457.0" prefHeight="200.0" prefWidth="200.0" />

         <Label fx:id="chordPlayed" layoutX="59.0" layoutY="427.0" text="Notes played correctly">
            <font>
               <Font size="14.0" />
            </font></Label>

      </children>

   </AnchorPane>

</VBox>
